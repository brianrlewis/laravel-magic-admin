{
    "name": "brianrlewis/laravel-magic-admin",
    "description": "Laravel integration for Magic Admin SDK",
    "type": "library",
    "keywords": ["laravel", "magic", "link", "fortmatic", "admin", "api", "sdk", "passwordless", "authentication"],
    "require": {
        "brianrlewis/magic-admin": "^1.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BrianRLewis\\LaravelMagicAdmin\\": "src"
        }
    },
    "homepage":"https://github.com/brianrlewis/laravel-magic-admin",
    "license": "MIT",
    "scripts": {
        "php-fix": [
            "php-cs-fixer fix"
        ]
    },
    "authors": [
        {
            "name": "Brian Lewis",
            "email": "brlewis23@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "BrianRLewis\\LaravelMagicAdmin\\ServiceProvider"
            ],
            "aliases": {
                "Magic": "BrianRLewis\\LaravelMagicAdmin\\Facade"
            }
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
